{
  "name": "ragbot-admin-console",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=frontend\" \"npm run dev --workspace=backend\"",
    "build": "npm run build --workspace=backend && npm run build --workspace=frontend",
    "build:client": "npm run build --workspace=frontend",
    "build:server": "npm run build --workspace=backend",
    "start": "npm run start --workspace=backend",
    "preview": "npm run preview --workspace=frontend",
    "serve-dist": "npm run serve-dist --workspace=frontend",
    "install:all": "npm install && npm install --workspace=frontend && npm install --workspace=backend",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "reembed": "npm run reembed --workspace=backend",
    "validate-embeddings": "npm run validate-embeddings --workspace=backend",
    "setup-vector-index": "npm run setup-vector-index --workspace=backend"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.2.0"
  }
}
